{
  "expo": {
    "name": "Oyun",
    "slug": "oyun",
    "version": "0.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "oyun",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android", "web"],
    "githubUrl": "https://github.com/Sanal/Oyun",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
